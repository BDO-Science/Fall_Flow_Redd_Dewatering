mutate(Spawn_Flows = zoo::na.approx(Spawn_Flows), EmergDate = Date + incubDays) %>%
mutate(Dewater = sapply(EmergDate, function(edate) {
if (!is.na(edate)) {
min(Spawn_Flows[Date <= edate & Date >= Date], na.rm = TRUE)
} else {
NA
}
}))
